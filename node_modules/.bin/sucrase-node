#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/sucrase/bin/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/sucrase/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/node_modules:/home/dankyle/Documents/OWN_PROJ/node_modules:/home/dankyle/Documents/node_modules:/home/dankyle/node_modules:/home/node_modules:/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/sucrase/bin/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/sucrase/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/node_modules:/home/dankyle/Documents/OWN_PROJ/node_modules:/home/dankyle/Documents/node_modules:/home/dankyle/node_modules:/home/node_modules:/node_modules:/home/dankyle/Documents/OWN_PROJ/professionel/my_portfolio/sipress-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase-node" "$@"
fi
